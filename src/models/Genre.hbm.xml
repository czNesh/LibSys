<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class dynamic-insert="false" dynamic-update="false" mutable="true" name="models.Genre" optimistic-lock="version" polymorphism="implicit" select-before-update="false">
        <id name="id" type="long" unsaved-value="null">
            <generator class="identity"/>
        </id>
        <property name="genreType" not-null="true">
            <type name="org.hibernate.type.EnumType">
                <param name="enumClass">models.GenreType</param>
                <param name="type">12</param>
            </type>
        </property>  
    </class>
</hibernate-mapping>
